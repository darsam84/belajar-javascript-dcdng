const result = [4, 5, 6, ...oneTwoThree, ...sevenEightNine];

console.log(result);
A
[1, 2, 3, 4, 5, 6, 7, 8, 9]

B
[4, 5, 6, 1, 2, 3, 7, 8, 9]

C
[4, 5, 6, 7, 8, 9, 1, 2, 3]

D
Error
